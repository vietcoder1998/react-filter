{
  "name": "react-ts-filter",
  "version": "0.1.3",
  "main": "dist/index.js",
  "dependencies": {
    "@reduxjs/toolkit": "^1.9.1",
    "@sgratzl/chartjs-chart-boxplot": "^4.1.1",
    "@swc/core": "^1.3.73",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^13.2.1",
    "@types/redux-logger": "^3.0.9",
    "moment": "^2.29.4",
    "react": "^17.0.2",
    "tsup": "^7.1.0",
    "universal-cookie": "^4.0.4",
    "web-vitals": "^2.1.0",
    "yup": "^1.0.2"
  },
  "scripts": {
    "start": "env-cmd craco start",
    "build:lib": "tsup src/index.ts",
    "build": "env-cmd craco --max_old_space_size=1536 build",
    "build:production": "GENERATE_SOURCEMAP=false env-cmd craco --max_old_space_size=1536 build",
    "test": "env-cmd craco test --watchAll=false --passWithNoTests",
    "test:ci": "env-cmd craco test --watchAll=false --passWithNoTests --ci --coverage --reporters=default --reporters=jest-junit",
    "eject": "react-scripts eject",
    "lint": "eslint --ext js,jsx,ts,tsx --max-warnings=0 src/",
    "lint:ci": "eslint --ext js,jsx,ts,tsx src/",
    "format": "prettier --write 'src/**/*.{js,jsx,ts,tsx,css,json}' --config ./.prettierrc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "axios": "axios/dist/node/axios.cjs"
    },
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest",
      "^.+\\.scss$": "jest-scss-transform"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!@shotgunjed)/"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@types/react": "^17.0.2",
    "@types/webpack-env": "^1.18.0",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "babel-jest": "^29.5.0",
    "craco-alias": "^3.0.1",
    "env-cmd": "^10.1.0",
    "eslint": "^8.32.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^24.0.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "husky": "^8.0.3",
    "identity-obj-proxy": "^3.0.0",
    "jest-junit": "^15.0.0",
    "jest-scss-transform": "^1.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "sass": "^1.57.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,json}": [
      "yarn lint"
    ]
  }
}
